  keywords = {Adult,Female,Histocytochemistry,Humans,L-Lactate Dehydrogenase,Male,Muscles,Physical Exertion,Sex Factors},
  pages = {30--35},
  groups = {Hommes-Femmes,Hommes-Femmes},
