@article{cutti_soft_2005,
  title = {Soft Tissue Artefact Assessment in Humeral Axial Rotation},
  volume = {21},
  issn = {0966-6362},
  doi = {10.1016/j.gaitpost.2004.04.001},
  abstract = {The accuracy of upper-limb kinematic data acquired from optoelectronic systems with retro-reflective markers is poor, mainly due to soft tissue artefact (STA). For the upper-arm, humeral internal/external rotation (HIER) is the movement most affected by STA, which is measured as a percentile fraction (K) of the effective humeral axial rotation performed. The aim of this work was to quantify STA during HIERs, with independently varying attitude of the humerus and elbow flexion, and to test the possibility of estimating its mean value over the tested upper-limb orientations using one simple trial. Six able-bodied subjects performed a series of HIERs in combination with elbow flexion for different humeral planes and degrees of elevation. During the trials the instantaneous attitudes of two humeral anatomical frames were compared, one being affected by the STA to be measured, and the other assumed as the gold standard. K was found to range from 20\% to 48\% of the effective humeral axial rotation performed, depending on the subject, humeral attitude and elbow flexion. These last two factors comparably affect STA and resulted in mean K coefficients of variation among the subjects of about 9\% and 7\%, respectively. Common patterns of K with elbow flexion and humerus elevation are discussed. The data also show that the mean of K of a subject is very close to the value assessed in a specific upper-limb configuration consistent among the subjects. This result from this study could be used to build up a time-saving STA compensation procedure suitable for clinical applications.},
  language = {eng},
  timestamp = {2017-02-17T17:56:19Z},
  number = {3},
  journal = {Gait Posture},
  author = {Cutti, Andrea Giovanni and Paolini, Gabriele and Troncossi, Marco and Cappello, Angelo and Davalli, Angelo},
  month = apr,
  year = {2005},
  keywords = {Adult,Algorithms,Artifacts,Biomechanical Phenomena,Humans,Humerus,Male,Movement,Muscle; Skeletal,Photogrammetry,Range of Motion; Articular,Rotation},
  pages = {341--349},
  groups = {Hommes-Femmes,Hommes-Femmes},
  pmid = {15760751}
}

