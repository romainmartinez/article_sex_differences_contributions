  author = {Ostergren, Per-Olof and Hanson, Bertil S. and Balogh, Istvan and Ektor-Andersen, John and Isacsson, Agneta and Orbaek, Palle and Winkel, J{\"o}rgen and Isacsson, Sven-Olof and {Malm{\"o} Shoulder Neck Study Group}},
  month = sep,
