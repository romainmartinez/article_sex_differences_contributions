  timestamp = {2016-08-09T15:58:21Z},
  number = {3},
  journal = {Scand J Work Environ Health},
  author = {{van Rijn}, Rogier M. and Huisstede, Bionka Ma and Koes, Bart W. and Burdorf, Alex},
