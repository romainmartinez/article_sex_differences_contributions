  keywords = {Aged,Female,Humans,Incidence,Male,Middle Aged,Neck Pain,Occupational Diseases,Occupational Exposure,Pain Measurement,Posture,Prevalence,Prospective Studies,Psychology; Social,Risk Factors,Sex Factors,Shoulder Pain,Stress; Mechanical,Sweden,Workplace},
