  month = may,
  year = {2000},
  keywords = {Adenosine Triphosphatases,Adult,Anthropometry,Biopsy; Needle,Body Composition,Female,Humans,Male,Muscle Fibers; Fast-Twitch,Muscle Fibers; Slow-Twitch,Muscle; Skeletal,Myosin Heavy Chains,Reference Values,Sex Factors},
  pages = {623--629},
