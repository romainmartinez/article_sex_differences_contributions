  year = {1995},
  keywords = {Adult,Analysis of Variance,Biomechanical Phenomena,Energy Metabolism,Exercise Test,Female,Heart Rate,Humans,Occupational Therapy,Oxygen Consumption,Reference Values,Sex Factors,Work Capacity Evaluation},
  pages = {405--411},
